#: kio_nfs.cpp:151
#, kde-format
msgid "Cannot find an NFS version that host '%1' supports"
msgstr "'%1' என்ற புரவன் ஆதரிக்கும் NFS பதிப்பை கண்டுபிடிக்க முடியவில்லை"

#: kio_nfs.cpp:344
#, kde-format
msgid "The NFS protocol requires a server host name."
msgstr "NFS நெறிமுறைக்கு சேவையகத்தின் புரவன் பெயர் தேவை."

#: kio_nfs.cpp:647
#, kde-format
msgid "Empty destination"
msgstr "காலியான செருமிடம்"

#: kio_nfs.cpp:739
#, kde-format
msgid "RPC error %1, %2"
msgstr "RPC சிக்கல் %1, %2"

#: kio_nfs.cpp:775
#, kde-format
msgid "Filename too long"
msgstr "கோப்புப்பெயர் மிக நீளமாக உள்ளது"

#: kio_nfs.cpp:780
#, kde-format
msgid "Disk quota exceeded"
msgstr "வட்டில் உங்கள் பங்கு மீறப்பட்டுள்ளது"

#: kio_nfs.cpp:784
#, kde-format
msgid "NFS error %1, %2"
msgstr "NFS சிக்கல் %1, %2"

#: nfsv3.cpp:436 nfsv3.cpp:587
#, kde-format
msgid "Unknown target"
msgstr "தெரியாத இலக்கு"

