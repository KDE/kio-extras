#: thumbnail.cpp:211
#, kde-format
msgid "No MIME Type specified."
msgstr "No se ha especificado el tipo MIME."

#: thumbnail.cpp:218
#, kde-format
msgid "No or invalid size specified."
msgstr "No se ha especificado el tamaño o no es válido."

#: thumbnail.cpp:241
#, kde-format
msgid "Cannot create thumbnail for directory"
msgstr "No se ha podido crear la miniatura del directorio"

#: thumbnail.cpp:252
#, kde-format
msgid "No plugin specified."
msgstr "No se ha especificado ningún complemento."

#: thumbnail.cpp:257
#, kde-format
msgid "Cannot load ThumbCreator %1"
