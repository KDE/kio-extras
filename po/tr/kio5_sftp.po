#: kio_sftp.cpp:616
#, kde-kuit-format
msgid "Opening SFTP connection to host %1:%2"
msgstr "%1 makinesine SFTP bağlantısı açılıyor: %2"

#: kio_sftp.cpp:656
#, kde-format
msgid "Could not get server public key type name"
msgstr "Sunucu genel anahtar tür adı alınamadı"

#: kio_sftp.cpp:669
#, kde-format
