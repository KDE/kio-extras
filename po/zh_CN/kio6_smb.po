msgstr "<qt>请输入 <b>%1</b> 的身份验证信息</qt>"

#: kio_smb_auth.cpp:54
#, kde-format
msgid ""
"Please enter authentication information for:\n"
"Server = %1\n"
"Share = %2"
msgstr ""
"请输入身份验证信息：\n"
