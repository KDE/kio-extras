msgstr "„Apple“ ierīces"

#: kio_afc.cpp:181 kio_afc.cpp:402
#, kde-format
msgctxt "Link to folder with files stored inside apps"
msgid "Apps"
msgstr "Programmas"

#: kio_afc.cpp:606
#, kde-format
msgid "Cannot read without opening file first"
msgstr "Nevar nolasīt, datni vispirms neatverot"

#: kio_afc.cpp:626
#, kde-format
msgid "Cannot seek without opening file first"
msgstr "Nevar meklēt celiņā, datni vispirms neatverot"

#: kio_afc.cpp:654
#, kde-format
msgid "Cannot write without opening file first"
msgstr "Nevar rakstīt, datni vispirms neatverot"

#: kio_afc.cpp:806
#, kde-format
msgid "Cannot rename between devices."
msgstr "Nevar pārdēvēt starp ierīcēm."
