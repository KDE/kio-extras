msgid "Search Folder"
msgstr "საქაღალდის ძებნა"

#: kio_filenamesearch.cpp:276
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid search location\n"
"Trying to search the /proc directory"
msgstr ""
"არასწორი ძებნის მდებარეობა\n"
"ვცდი მოვძებნო საქაღალდეში /proc"

#: kio_filenamesearch.cpp:301
#, kde-format
msgctxt "@info:status"
msgid ""
"Invalid search query: '%1'\n"
"Expected a regular expression: %2"
